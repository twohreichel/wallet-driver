{
  "name": "twoh/wallet-driver",
  "description": "This project includes a basic Driver to interact with different Walletsystems like Apple and Google.",
  "version": "1.0.2",
  "keywords": [
    "php",
    "library",
    "wallet",
    "driver",
    "google",
    "apple"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "twoh/validator": "^1",
    "twoh/logger": "^1",
    "google/apiclient": "^2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^11"
  },
  "autoload": {
    "psr-4": {
      "TWOH\\WalletDriver\\": "src/Classes"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.3"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Andreas Reichel",
      "email": "a.reichel91@outlook.com"
    }
  ]
}
